<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
              version="2.2">
    <managed-bean>
        <managed-bean-name>LoginManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.LoginManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RegisterManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.RegisterManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DashboardManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.DashboardManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>NavManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.NavManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SysAdminManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.SysAdminManagedBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>MyTasksManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.MyTasksManagedBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>CreateDotoManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.CreateDotoManagedBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>CreateCommunityManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.CreateCommunityManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>JoinCommunityManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.JoinCommunityManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>OpenDotosManagedBean</managed-bean-name>
        <managed-bean-class>hwr.sem4.csa.managedBeans.OpenDotosManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <converter>
        <converter-id>dotosConverter</converter-id>
        <converter-class>hwr.sem4.csa.converter.DotosConverter</converter-class>
    </converter>
</faces-config>